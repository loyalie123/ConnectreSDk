<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
		xmlns:app="http://schemas.android.com/apk/res-auto"
		android:layout_width="match_parent"
		android:layout_height="wrap_content">

	<androidx.appcompat.widget.AppCompatTextView
			android:id="@+id/headingHolderTV"
			android:layout_width="0dp"
			android:layout_height="wrap_content"
			android:layout_marginTop="25dp"
			android:text="Address"
			android:textColor="#b3262626"
			android:textSize="12sp"
			app:fontFamily="@font/sinkinsans_700bold"
			app:layout_constraintEnd_toEndOf="parent"
			app:layout_constraintStart_toStartOf="parent"
			app:layout_constraintTop_toTopOf="parent" />

	<androidx.appcompat.widget.AppCompatTextView
			android:id="@+id/headingMainTV"
			android:layout_width="0dp"
			android:layout_height="wrap_content"
			android:layout_marginTop="5dp"
			android:layout_marginEnd="3dp"
			android:ellipsize="end"
			android:lineSpacingExtra="4dp"
			android:textColor="#262626"
			android:textSize="11sp"
			app:fontFamily="@font/sinkinsans_400regular"
			app:layout_constraintEnd_toEndOf="parent"
			app:layout_constraintStart_toStartOf="parent"
			app:layout_constraintTop_toBottomOf="@+id/headingHolderTV" />

	<View
			android:layout_width="0dp"
			android:layout_height="0.5dp"
			android:layout_marginTop="25dp"
			android:background="#1a000000"
			app:layout_constraintEnd_toEndOf="parent"
			app:layout_constraintStart_toStartOf="parent"
			app:layout_constraintTop_toBottomOf="@+id/headingMainTV" />

</androidx.constraintlayout.widget.ConstraintLayout>
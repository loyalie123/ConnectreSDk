<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
		xmlns:app="http://schemas.android.com/apk/res-auto"
		android:layout_width="match_parent"
		android:layout_height="35dp">

	<androidx.appcompat.widget.AppCompatImageView
			android:id="@+id/programStatusIV"
			android:layout_width="15dp"
			android:layout_height="15dp"
			android:src="@drawable/ic_done_black_24dp"
			app:layout_constraintBottom_toBottomOf="parent"
			app:layout_constraintStart_toStartOf="parent"

			app:layout_constraintTop_toTopOf="parent" />

	<androidx.appcompat.widget.AppCompatTextView
			android:id="@+id/nameTV"
			android:layout_width="0dp"
			android:layout_height="wrap_content"
			android:layout_marginStart="5dp"
			android:ellipsize="end"
			android:lineSpacingExtra="4.5dp"

			android:maxLines="2"
			android:text="Euniversalmente ricon sdfgfh fhfhhf fhfhhf f hfhhfh fhfhhf fhfhfhjfhjfh fdjdshsjsksjd fjffhfhf osciuto che"
			android:textColor="#B3262626"
			android:textSize="12sp"
			app:fontFamily="@font/sinkinsans_400regular"
			app:layout_constraintBottom_toBottomOf="parent"
			app:layout_constraintEnd_toStartOf="@+id/programStatusTV"
			app:layout_constraintStart_toEndOf="@+id/programStatusIV"
			app:layout_constraintTop_toTopOf="parent" />

	<androidx.appcompat.widget.AppCompatTextView
			android:id="@+id/programStatusTV"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:layout_marginEnd="20dp"
			android:text="Accepted"
			android:textColor="#262626"
			android:textSize="11sp"
			app:fontFamily="@font/sinkinsans_500medium"
			app:layout_constraintBottom_toBottomOf="parent"
			app:layout_constraintEnd_toEndOf="parent"
			app:layout_constraintStart_toEndOf="@+id/nameTV"
			app:layout_constraintTop_toTopOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>